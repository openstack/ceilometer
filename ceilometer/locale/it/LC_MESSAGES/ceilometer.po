msgid "Ignoring instance %(name)s: %(error)s"
msgstr "Si sta ignorando l'istanza %(name)s: %(error)s"

#, python-format
msgid "Ignoring loadbalancer %(loadbalancer_id)s"
msgstr "Ignora loadbalancer %(loadbalancer_id)s"

