msgid "Ignoring instance %(name)s: %(error)s"
msgstr "Ignorando instância %(name)s: %(error)s"

#, python-format
msgid "Ignoring loadbalancer %(loadbalancer_id)s"
msgstr "Ignorando balanceador de carga %(loadbalancer_id)s"

#, python-format
msgid "Ignoring pool %(pool_id)s"
msgstr "Ignorando pool %(pool_id)s"

#, python-format
msgid ""
"Invalid YAML syntax in Definitions file %(file)s at line: %(line)s, column: "
"%(column)s."
